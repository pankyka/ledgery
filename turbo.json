{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "publish": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {},
    "test": {},
    "test:e2e": {}
  }
}
